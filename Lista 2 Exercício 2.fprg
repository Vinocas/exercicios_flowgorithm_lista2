    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="2.&#9;Maior entre dois n&#250;meros: Ler dois n&#250;meros inteiros, quaisquer e mostrar na tela uma mensagem indicando qual &#233; o maior, ou se s&#227;o iguais."/>
            <declare name="n1, n2" type="Integer" array="False" size=""/>
            <output expression="&quot;Insira o primeiro n&#250;mero inteiro:&quot;" newline="True"/>
            <input variable="n1"/>
            <output expression="&quot;Insira o segundo n&#250;mero inteiro:&quot;" newline="True"/>
